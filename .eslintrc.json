{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "standard"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "semi": "off",
        "quotes": "off",
        "no-extra-semi": "off",
        "no-undef-init": "off",
        "no-unused-vars": "warn",
        "no-trailing-spaces": "warn",
        "comma-dangle": "off",
        "eol-last": "warn",
        "array-element-newline": "off",
        "curly": "off",
        "camelcase": "off",
        "indent": [
            "error",
            2
        ],
        "space-before-function-paren": [
            "error",
            {
                "anonymous": "ignore",
                "named": "ignore",
                "asyncArrow": "ignore"
            }
        ]
    }
}